        android:paddingTop="@dimen/paddingTop"
        />

    <WebView
        android:layout_width="match_parent"
        android:layout_height="155dp"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="130dp"
        android:layout_weight="0.49" />
    />

